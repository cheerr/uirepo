<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ui_typeface">
        <attr name="ui_typeface">
            <enum name="lantinghei" value="1" />
            <enum name="gotham_light" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="feature">
        <attr name="listview">
            <enum name="SmoothListFeature" value="0" />
            <enum name="PullToReFreshFeature" value="1" />
            <enum name="PullTipFeature" value="2" />
            <enum name="SecPullFeature" value="3" />
        </attr>
        <attr name="scrollview">
            <enum name="PullToRefreshFeature" value="1" />
            <enum name="PullTipFeature" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="PathView">
        <attr name="pathColor" format="color" />
        <attr name="pathWidth" format="float" />
        <attr name="svg" format="reference" />
    </declare-styleable>
</resources>